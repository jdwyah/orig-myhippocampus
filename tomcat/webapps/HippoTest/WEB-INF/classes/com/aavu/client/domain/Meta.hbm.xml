<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.aavu.client.domain">

	<class name="Meta" table="Meta">
		<meta attribute="generated-class">com.aavu.client.domain.generated.AbstractMeta</meta>
		<meta attribute="scope-class">public abstract</meta>

		<id name="id" type="long" column="meta_id">
			<generator class="native"></generator>
		</id>

		<property name="name" type="string" column="name">
			<meta attribute="field-description">The type.</meta>
			<meta attribute="use-in-equals">true</meta>
		</property>

		<property name="type" type="string" column="type">
			<meta attribute="field-description">The type of the value</meta>
			<meta attribute="use-in-equals">true</meta>
		</property>

	</class>

</hibernate-mapping>